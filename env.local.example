# Local Development Environment Variables
# Copy this file to .env.local and update the values for your local setup

# Database URL for local development
# You can use a local PostgreSQL instance or a separate Neon database for development
DATABASE_URL="postgresql://your-local-username:your-local-password@localhost:5432/iyn_local?schema=public"

# JWT Secret for local development (change this to a secure random string)
JWT_SECRET="local-development-jwt-secret-change-this-to-something-secure"

# NextAuth configuration for local development
NEXTAUTH_SECRET="local-development-nextauth-secret-change-this"
NEXTAUTH_URL="http://localhost:3000"

# Environment indicator
NODE_ENV="development"
